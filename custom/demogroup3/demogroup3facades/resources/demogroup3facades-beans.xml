<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;com.hybris.demogroup3.facades.product.data.GenderData>"/>
		<property name="ukuran" type="String"/>
		<property name="warna" type="String"/>
		<property name="merk" type="String"/>
		<property name="attribute" type="String"/>
		<property name="keterangan" type="String"/>


	</bean>

	<bean class="com.hybris.demogroup3.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>

	<bean class="de.hybris.platform.commercefacades.user.data.RegisterData">
		<property name="id" type="String"/>
	</bean>

	<bean class="com.hybris.demogroup3.facades.product.data.KasurVariantProductData">
		<property name="name" type="String"/>
		<property name="code" type="String"/>
		<property name="merk" type="String"/>
		<property name="ukuran" type="String"/>
		<property name="keterangan" type="String"/>
		<property name="strPrice" type="String"/>
		<property name="priceData" type="de.hybris.platform.commercefacades.product.data.PriceData"/>
		<property name="imgUrl" type="String"/>
	</bean>

	<bean class="com.hybris.demogroup3.facades.product.data.ShofaVariantProductData">
		<property name="name" type="String"/>
		<property name="code" type="String"/>
		<property name="merk" type="String"/>
		<property name="warna" type="String"/>
		<property name="keterangan" type="String"/>
		<property name="strPrice" type="String"/>
		<property name="priceData" type="de.hybris.platform.commercefacades.product.data.PriceData"/>
		<property name="imgUrl" type="String"/>
	</bean>
</beans>
